.all {
  margin-top: 10rpx;
}

.all .bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 10rpx 20rpx;
  padding: 10rpx;
}

.all .bar image {
  width: 50rpx;
  height: 30rpx;
}

.all .week {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  margin: 10rpx;
  border-radius: 10px;
  background-color: #acd;
}

.all .days {
  margin: 20rpx;
  padding: 10rpx;
  border-radius: 10px;
  background-color: #acd;
}

.all .columns {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.all .columns .rows {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.all .columns .rows .cell {
  width: 84rpx;
  height: 57rpx;
  margin: 3rpx;
  text-align: center;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.count .daynumber {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.count .daynumber .day {
  margin-top: 50rpx;
}

.count {
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  flex-direction: column;
  justify-content: center;
  background-color: #acd;
  align-items: center;
}

.count .number {
  color: red;
  font-size: 40rpx;
  background-color: #fff;
  width: 100rpx;
  height: 60rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 20rpx;
}

.count text {
  margin: 10rpx;
}

.section {
  font-size: 10px;
  white-space: 0em;
  background-color: #acd;
  float: left;
  flex-direction: row;
  word-wrap: normal;
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  justify-content: center;
  align-items: center;
}

.leave {
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: left;
  border-radius: 10px;
  flex-direction: column;
  justify-content: left;
  background-color: #acd;
  align-items: center;
}
